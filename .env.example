# Database connection string (replace placeholders with actual values)
DATABASE_URL=postgresql://<user>:<password>@<host>:5432/<database>

# Secret key for signing JWTs (replace with a secure, randomly generated key)
SECRET_KEY=your_secret_key

# Algorithm used for signing JWT tokens
ALGORITHM=HS256

# Expiration time for access tokens in minutes
ACCESS_TOKEN_EXPIRE_MINUTES=30
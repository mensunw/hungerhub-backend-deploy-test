{
  "version": 2,
  "builds": [
    {
      "src": "fastapi-backend/app/main.py",
      "use": "@vercel/python",
      "config": {
        "installCommand": "pip install -r fastapi-backend/requirements.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "fastapi-backend/app/main.py"
    }
  ]
}